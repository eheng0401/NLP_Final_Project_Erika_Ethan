{{begin-eqn}} {{eqn | l = \map {f'} \xi | r = \lim_{h \mathop \to 0} \frac {\map f {\xi + h} - \map f \xi} h | c = {{Defof|Derivative}} }} {{eqn | r = \lim_{h \mathop \to 0} \frac {\paren {\map j {\xi + h} + \map k {\xi + h} } - \paren {\map j \xi + \map k \xi } } h | c = }} {{eqn | r = \lim_{h \mathop \to 0} \frac {\map j {\xi + h} + \map k {\xi + h} - \map j \xi - \map k \xi} h | c = }} {{eqn | r = \lim_{h \mathop \to 0} \frac {\paren {\map j {\xi + h} - \map j \xi} + \paren {\map k {\xi + h} - \map k \xi} } h | c = }} {{eqn | r = \lim_{h \mathop \to 0} \paren {\frac {\map j {\xi + h} - \map j \xi} h + \frac {\map k {\xi + h} - \map k \xi} h} | c = }} {{eqn | r = \lim_{h \mathop \to 0} \frac {\map j {\xi + h} - \map j \xi} h + \lim_{h \mathop \to 0} \frac {\map k {\xi + h} - \map k \xi} h | c = }} {{eqn | r = \map {j'} \xi + \map {k'} \xi | c = {{Defof|Derivative}} }} {{end-eqn}} {{qed}}	0
