Let: {{begin-eqn}} {{eqn | n = a | l = A | o = := | r = \sum_{i \mathop = a}^b \sum_{j \mathop = a}^i \sum_{k \mathop = a}^j x_i x_j x_k | c = }} {{eqn | r = \sum_{a \mathop \le i \mathop \le j \mathop \le k \mathop \le b} x_i x_j x_k | c = }} {{eqn | n = b | r = \sum_{i \mathop = a}^b \sum_{j \mathop = i}^b \sum_{k \mathop = j}^b x_i x_j x_k | c = }} {{eqn | n = c | r = \sum_{i \mathop = a}^b \sum_{j \mathop = i}^b \sum_{k \mathop = i}^j x_i x_j x_k | c = }} {{eqn | n = d | r = \sum_{i \mathop = a}^b \sum_{j \mathop = a}^i \sum_{k \mathop = i}^j x_i x_j x_k | c = }} {{eqn | n = e | r = \sum_{i \mathop = a}^b \sum_{j \mathop = a}^i \sum_{k \mathop = i}^b x_i x_j x_k | c = }} {{end-eqn}} Also, let: {{begin-eqn}} {{eqn | l = S_1 | o = := | r = \sum_{i \mathop = a}^b x_i | c = }} {{eqn | l = S_2 | o = := | r = \sum_{i \mathop = a}^b {x_i}^2 | c = }} {{eqn | l = S_3 | o = := | r = \sum_{i \mathop = a}^b {x_i}^3 | c = }} {{end-eqn}} Hence: {{begin-eqn}} {{eqn | l = 2 A | r = \sum_{i \mathop = a}^b \sum_{j \mathop = i}^b \sum_{k \mathop = j}^b x_i x_j x_k + \sum_{i \mathop = a}^b \sum_{j \mathop = i}^b \sum_{k \mathop = i}^j x_i x_j x_k | c = $(b) + (c)$ }} {{eqn | r = \sum_{i \mathop = a}^b \sum_{j \mathop = i}^b \left({\sum_{k \mathop = j}^b x_i x_j x_k + \sum_{k \mathop = i}^j x_i x_j x_k}\right) | c = }} {{eqn | r = \sum_{i \mathop = a}^b \sum_{j \mathop = i}^b \left({\sum_{k \mathop = i}^b x_i x_j x_k + x_i x_j x_j}\right) | c = }} {{eqn | r = \sum_{i \mathop = a}^b x_i \left({\sum_{j \mathop = i}^b x_j}\right)^2 + \sum_{i \mathop = a}^b x_i \left({\sum_{j \mathop = i}^b {x_j}^2}\right) | c = }} {{end-eqn}} Let: {{begin-eqn}} {{eqn | l = A_1 | o = := | r = \sum_{i \mathop = a}^b x_i \left({\sum_{j \mathop = i}^b x_j}\right)^2 | c = }} {{eqn | r = \sum_{i \mathop = a}^b x_i \sum_{j \mathop = i}^b x_j \sum_{k \mathop = i}^b x_k | c = }} {{eqn | r = \sum_{i \mathop = a}^b \sum_{j \mathop = i}^b \sum_{k \mathop = i}^b x_i x_j x_k | c = }} {{eqn | l = A_3 | o = := | r = \